<!DOCTYPE html>
<html>
<head>
    <title>{% block title %}DABSquaredPushNotificationsBundle{% endblock %}</title>
    <link href="{{ asset("bundles/dabsquaredpushnotifications/css/bootstrap.css") }}" rel="stylesheet" />
</head>
<body>
<div class="navbar navbar-fixed-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path("dabsquared_push_notifications_dashboard") }}">DABSquaredPushNotificationsBundle</a>

        </div>
        <div class="navbar-collapse collapse">
            <form class="navbar-form navbar-right" role="form">
                <div class="form-group">
                    <input type="text" placeholder="Email" class="form-control">
                </div>
                <div class="form-group">
                    <input type="password" placeholder="Password" class="form-control">
                </div>
                <button type="submit" class="btn btn-success">Sign in</button>
            </form>
        </div><!--/.navbar-collapse -->
    </div>
</div>

<div class="navbar">
    <div class="navbar-inner">
    </div>
</div>
<div class="container">
    {% block content %}{% endblock %}
</div>

{% block javascripts %}
    <script src="{{ asset("bundles/dabsquaredpushnotifications/js/jquery.js") }}" type="text/javascript"></script>
    <script src="{{ asset("bundles/dabsquaredpushnotifications/js/bootstrap.js") }}" type="text/javascript"></script>
{% endblock %}
</body>
</html>