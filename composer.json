    "type": "symfony-bundle",
    "description": "Push notifications/messages for mobile devices",
    "keywords": ["push", "notification", "bundle", "gcm", "c2dm", "ios", "apns", "blackberry", "android"],
    "homepage": "https://github.com/DABSquared/DABSquaredPushNotificationsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Brooks",
            "email": "daniel_brooks@dabsquared.com",
            "homepage": "http://www.dabsquared.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "friendsofsymfony/rest-bundle": "*",
        "symfony/framework-bundle": ">=2.1",
        "doctrine/common": ">=2.3",
        "jms/di-extra-bundle": ">=1.1",
